game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-643, 23, -289)
end)

MainSection:NewButton("(Spot-2)", "()", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-968, 22, -406)
end)
